<Window x:Class="quiz_resolver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:quiz_resolver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400">

    <Grid>

        <Button Content="Start" Click="Button_Click" Margin="10,350,10,10" Width="300"/>


        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="20,20,20,100"/>
    </Grid>

</Window>


